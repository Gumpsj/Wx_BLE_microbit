
.rootWrap {
  width: 100%;
  height: 100vh;
  overflow: hidden;
}

canvas {
  width: 100%;
  height: 100%;
}


#rocker {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 200rpx;
  height: 200rpx;
  background: yellowgreen;
  border-radius: 50%;
  text-align: center;
  line-height: 200rpx;
}

.Btn {
  position: absolute;
  bottom: 140rpx;
  left: 50%;
  transform: translateX(-50%);
  font-size: 15px;
  padding: 16rpx 60rpx;
  background: rgb(243, 153, 16);
  color: #fff;
  font-weight: 700;
  border-radius: 6rpx;
}